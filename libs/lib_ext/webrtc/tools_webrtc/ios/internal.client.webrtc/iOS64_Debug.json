{
  "comments": [
    "Tests for 64-bit iOS devices."
  ],
  "xcode version": "8.0",
  "gn_args": [
    "goma_dir=\"$(goma_dir)\"",
    "is_component_build=false",
    "is_debug=true",
    "target_cpu=\"arm64\"",
    "target_os=\"ios\"",
    "use_goma=true"
  ],
  "env": {
    "MAC_TOOLCHAIN_REVISION": "8E2002-1"
  },
  "use_analyze": false,
  "additional_compile_targets": [ "all" ],
  "configuration": "Debug",
  "bucket": "bling-archive",
  "tests": [
    {
      "app": "modules_unittests",
      "device type": "iPhone 6s",
      "os": "10.0.1"
    }
  ]
}
